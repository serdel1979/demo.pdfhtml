@model PdfViewModel

<!DOCTYPE html>
<html>
<head>
    <title>PDF Display</title>
</head>
<body>
    <div>
        <h2>Text Content</h2>
        <p>@Html.Raw(Model.TextContent)</p>

        <h2>Images</h2>
        @foreach (var imagePath in Model.ImagePaths)
        {
            <img src="@imagePath" alt="PDF Image" />
        }
    </div>
</body>
</html>
